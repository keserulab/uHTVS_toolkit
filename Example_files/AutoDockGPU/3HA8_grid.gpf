npts 60 50 50                        # num.grid points in xyz
gridfld 3HA8_empty_OK.maps.fld       # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C HD N NA OA SA     # receptor atom types
ligand_types A C NA OA N HD SA S P I Cl Br F  # ligand atom types
